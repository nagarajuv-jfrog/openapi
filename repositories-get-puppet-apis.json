{
    "openapi": "3.0.0",
    "info": {
        "title": "Repositories - Get Puppet Apis",
        "description": "Provides endpoints for querying Puppet module releases and specific release information within a specified repository. These APIs support pagination and authentication where necessary.",
        "version": "1.0.0"
    },
    "paths": {
        "/artifactory/api/puppet/{repoKey}/v3/releases": {
            "get": {
                "summary": "Get Puppet Releases",
                "description": "Returns a list of all Puppet releases hosted by the specified repository. Results are paginated and all of the parameters in the pagination section are optional.",
                "operationId": "getPuppetReleases",
                "parameters": [
                    {
                        "name": "repoKey",
                        "in": "path",
                        "required": true,
                        "description": "The key of the repository",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "description": "Maximum number of releases to return",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "required": false,
                        "description": "Offset for pagination",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "total": {
                                            "type": "integer"
                                        },
                                        "limit": {
                                            "type": "integer"
                                        },
                                        "offset": {
                                            "type": "integer"
                                        },
                                        "current": {
                                            "type": "string"
                                        },
                                        "next": {
                                            "type": "string"
                                        },
                                        "previous": {
                                            "type": "string"
                                        },
                                        "results": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "uri": {
                                                        "type": "string"
                                                    },
                                                    "version": {
                                                        "type": "string"
                                                    },
                                                    "module": {
                                                        "type": "object",
                                                        "properties": {
                                                            "uri": {
                                                                "type": "string"
                                                            },
                                                            "name": {
                                                                "type": "string"
                                                            }
                                                        }
                                                    },
                                                    "metadata": {
                                                        "type": "object"
                                                    },
                                                    "tags": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "string"
                                                        }
                                                    },
                                                    "supported": {
                                                        "type": "boolean"
                                                    },
                                                    "file_size": {
                                                        "type": "integer"
                                                    },
                                                    "file_md5": {
                                                        "type": "string"
                                                    },
                                                    "downloads": {
                                                        "type": "integer"
                                                    },
                                                    "readme": {
                                                        "type": "string"
                                                    },
                                                    "changelog": {
                                                        "type": "string"
                                                    },
                                                    "license": {
                                                        "type": "string"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    },
                                                    "deleted_at": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "security": []
            }
        },
        "/artifactory/api/puppet/{repoKey}/v3/releases/{user}-{module}-{version}": {
            "get": {
                "summary": "Get Puppet Release Info",
                "description": "Returns information about the specific Puppet module's release.",
                "operationId": "getPuppetReleaseInfo",
                "parameters": [
                    {
                        "name": "repoKey",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "user",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "module",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "version",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "uri": {
                                            "type": "string"
                                        },
                                        "version": {
                                            "type": "string"
                                        },
                                        "module": {
                                            "type": "object",
                                            "properties": {
                                                "uri": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "metadata": {
                                            "type": "object"
                                        },
                                        "tags": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "supported": {
                                            "type": "boolean"
                                        },
                                        "file_size": {
                                            "type": "integer"
                                        },
                                        "file_md5": {
                                            "type": "string"
                                        },
                                        "downloads": {
                                            "type": "integer"
                                        },
                                        "readme": {
                                            "type": "string"
                                        },
                                        "changelog": {
                                            "type": "string"
                                        },
                                        "license": {
                                            "type": "string"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "deleted_at": {
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        }
    },
    "tags": []
}